{
    "manifest_version": 3,
    "name": "catpubparser",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "unlimitedStorage",
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "minimum_chrome_version": "92",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }